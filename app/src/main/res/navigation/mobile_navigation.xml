<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.maverick.iotsocket.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.maverick.iotsocket.ui.controlcenter.ControlCenterFragment"
        android:label="@string/title_control_center"
        tools:layout="@layout/fragment_control_center" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.maverick.iotsocket.ui.commands.CommandsFragment"
        android:label="@string/title_commands"
        tools:layout="@layout/fragment_commands" />
</navigation>